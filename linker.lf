MEMORY {
    IRAM : ORIGIN = 0x4037C000, LENGTH = 0x40000
    DRAM : ORIGIN = 0x3FC80000, LENGTH = 0x40000
    FLASH : ORIGIN = 0x42000000, LENGTH = 8M
}

SECTIONS {
    .text : {
        *(.text .text.*)
    } > FLASH

    .rodata : {
        *(.rodata .rodata.*)
    } > FLASH

    .data : {
        *(.data .data.*)
    } > DRAM

    .bss : {
        *(.bss .bss.*)
    } > DRAM

    .literal : {
        *(.literal .literal.*)
    } > FLASH
}